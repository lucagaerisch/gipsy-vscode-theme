{
  "name": "gipsy-vscode-theme",
  "displayName": "Gipsy",
  "description": "VS-Code theme!",
  "publisher": "lucagaerisch",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucagaerisch/gipsy-vscode-theme"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/gipsy.js",
  "contributes": {
    "commands": [],
    "themes": [
      {
        "label": "Gipsy Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/gipsy-dark.json"
      }
    ]
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "keywords": [
    "theme",
    "focus",
    "dark",
    "light"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "latest",
    "@types/mocha": "^10.0.10",
    "@types/node": "~16.18.121",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vscode/test-electron": "latest",
    "eslint": "^9.16.0",
    "glob": "^8.1.0",
    "mocha": "^10.8.2",
    "typescript": "^4.9.5"
  }
}
