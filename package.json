{
  "name": "gipsy-vscode-theme",
  "displayName": "Gipsy",
  "description": "VS-Code theme!",
  "publisher": "lucagaerisch",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [ "onStartupFinished" ],
  "main": "./out/gipsy.js",
  "contributes": {
    "commands": [ ],
    "themes": [
      {
        "label": "Gipsy Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/gipsy-dark.json"
      }
    ]
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "keywords": [
    "theme",
    "focus",
    "dark",
    "light"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
